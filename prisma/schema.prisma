generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Task {
  id        Int       @id @default(autoincrement())
  name      String
  userId    Int
  createdAt DateTime  @default(now())
}

model User {
  id        Int       @id @default(autoincrement())
  name      String
}